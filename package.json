{
  "name": "nvr-wiki",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "preparse": "(sed -i '1d' .\\NewVegasReloaded.dll.defaults.toml || findstr /V /R \"^#\" .\\NewVegasReloaded.dll.defaults.toml > .\\NewVegasReloaded.dll.defaults.toml.temp && move /y .\\NewVegasReloaded.dll.defaults.toml.temp .\\NewVegasReloaded.dll.defaults.toml) && (rm -rf .\\Main.md || del /s /q /f .\\Main.md) && (rm -rf .\\docs\\Shaders || del /s /q /f .\\docs\\Shaders >nul 2>nul)",
    "parse": "python create_md.py",
    "postparse": "markdownlint -f .\\docs\\",
    "github": "start https://github.com/dlpnd/nvr-wiki",
    "wiki": "start https://dlpnd.github.io/nvr-wiki"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/faster": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-search-algolia": "^3.7.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^2.0.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "markdownlint-cli": "^0.43.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=23.3.0"
  }
}